<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <rotate
        android:fromDegrees="90"
        android:pivotX="50%"
        android:pivotY="50%"
        android:toDegrees="-90">
        <shape android:shape="line">
            <stroke
                android:width="2dp"
                android:color="@color/dark_gray" />
        </shape>
    </rotate>
</selector>